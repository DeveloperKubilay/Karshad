{
    "Machines": [
        {
            "type": "Google",
            "ProjectName": "false-chat",
            "env": "env.json",
            "template": "template.json",
            "count": 5
        },
        {
            "type": "Azure",
            "envPrefix": "AZURE_",
            "count": 5
        }
